{% extends "SelnetTiendaOnlineBundle::template.html.twig" %}



{% block contenido %}

<div class="barra_spacer_3"></div>

<div class="row">
	<div class="col-md-10 col-md-offset-1">
		<div class="page-header pagina_header">
			<h2>Checkout!</h2>
		</div>
	</div>
</div>




<div class="row">
	<div class="col-md-10 col-md-offset-1">
		<div class="panel panel-default artsenal-panel "  style="margin-bottom: 0px; border-bottom-width: 0px;" >
			<div class="panel-body"></div>
			<div class="container-fluid">
				<div class="row">

					<div  class="col-md-12" >
						<div class="row  tab-holder">
							<div  class="col-md-6  "  style="text-align:center;" >
								<a href="#" class="tabs_tienda tabs_tienda_selected" onclick="mostrar_contenido(1)"  >Realizar Pago</a>
							</div>
							<div  class="col-md-6  "   style="text-align:center;" >
								<a href="#" class="tabs_tienda" onclick="mostrar_contenido(2)"  >Finalizacion</a>
							</div>							
						</div>
					</div>
				</div>
			</div>


		</div>
	</div>
</div>
<div class="row">
	<div class="col-md-10 col-md-offset-1">
		<div class="panel panel-default artsenal-panel" >
			<div class="panel-body"  id="contenido_cart" >
				<div class="container-fluid">
					


					<div id="contenido1" class="row contenido_tabs" >	
						<div  class="col-md-8  col-md-offset-2" >	
							<div class="barra_spacer_3"></div>	
							<div class="barra_spacer_3"></div>	
							<div class="page-header pagina_header">
								<h2  style="text-align:center;" >Solo un paso mas !</h2>
							</div>

							<p  class="form_parrafo" >En este momento su pedido esta en estado pendiente, una vez que realize el pago mediante PayPal, se le enviara un correo con los detalles de la entrega del producto solicitado.</p>
							
							<div class="barra_border"></div>	

							<div class="barra_spacer_3"></div>	



							<div class="row">
								<div class="col-sm-offset-3 col-sm-6">



									<form action="https://www.paypal.com/cgi-bin/webscr" method="post" target="_top">
										<input type="hidden" name="cmd" value="_xclick">
										<input type="hidden" name="amount" value="12.99">
										<input type="hidden" name="item_name" value="Compra en arsenal">
										<input type="hidden" name="currency_code" value="USD">
										<input type="hidden" name="encrypted" value="-----BEGIN PKCS7----------END PKCS7-----
										">
										<input type="image" src="https://www.paypalobjects.com/es_XC/i/btn/btn_buynowCC_LG.gif" border="0" name="submit" alt="PayPal, la forma más segura y rápida de pagar en línea.">
										<img alt="" border="0" src="https://www.paypalobjects.com/es_XC/i/scr/pixel.gif" width="1" height="1">
									</form>



								</div>	
							</div>
							<div class="barra_spacer_3"></div>	
							<div class="barra_spacer_3"></div>
							<div class="barra_spacer_3"></div>
							<div class="barra_spacer_3"></div>

						</div>
					</div>




					
					<div id="contenido2" class="row contenido_tabs" style="display:none;"   >	
						<div  class="col-md-8  col-md-offset-2" >	
							<div class="barra_spacer_3"></div>	
							<div class="barra_spacer_3"></div>	
							<div class="page-header pagina_header">
								<h2  style="text-align:center;" >Gracias por tu compra !</h2>
							</div>

							<div class="barra_border"></div>	

							<div class="barra_spacer_3"></div>	

							<div class="row">
								<div class="col-sm-offset-3 col-sm-6">
									<button onclick="window.location='{{  path('artsenal_index') }}'" class="btn btn-default pull-right btn_aquamarino"  style="width:100%;height:40px;" type="submit">Regresar al inicio</button>
								</div>	
							</div>
							<div class="barra_spacer_3"></div>	
							<div class="barra_spacer_3"></div>
							<div class="barra_spacer_3"></div>
							<div class="barra_spacer_3"></div>

						</div>
					</div>




				</div>

			</div>
		</div>
	</div>

</div>

<div class="barra_spacer_3"></div>	
<div class="barra_spacer_3"></div>	



{% endblock %}